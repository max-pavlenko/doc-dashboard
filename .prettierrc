{
  "printWidth": 130,
  "singleQuote": true,
  "bracketSpacing": true,
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "endOfLine": "auto",
  "trailingComma": "all",
  "arrowParens": "always",
  "bracketSameLine": true,
  "plugins": ["prettier-plugin-organize-attributes"],
  "attributeGroups": [
    "$ANGULAR_STRUCTURAL_DIRECTIVE",
    "^app",

    "$ANGULAR_ELEMENT_REF",
    "$ID",
    "$ANGULAR_ANIMATION",
    "$DEFAULT",
    "$DATA",

    "$ANGULAR_ANIMATION_INPUT",
    "$ANGULAR_INPUT",
    "$ANGULAR_TWO_WAY_BINDING",
    "$ANGULAR_OUTPUT"
  ]
}
